# Measure at RT 4K 200K
samplename BaTi0.93 at RT
runscan stth 2.75 5.2 50 time 455

samplename BaTi0.93 at 100K
hset /sample/tc1/sensor/setpoint1 100
hset /sample/tc1/sensor/setpoint2 100
drive tc2_driveable 100
wait 300

runscan stth 2.75 5.2 50 time 455

samplename BaTi0.93 at 250K
hset /sample/tc1/sensor/setpoint1 250
hset /sample/tc1/sensor/setpoint2 250
drive tc2_driveable 250
wait 300

runscan stth 2.75 5.2 50 time 455